{
    "manifest_version": 3,
    "name": "Flashback",
    "version": "1.0",
    "description": "Spotlight-like search for browser history",
    "permissions": [
        "history",
        "tabs",
        "sessions",
        "bookmarks"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+K",
                "mac": "Command+Shift+K"
            },
            "description": "Open Flashback Search"
        }
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    }
}